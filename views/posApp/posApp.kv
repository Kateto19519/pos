<posApp>:
   BoxLayout:
      orientation:'horizontal'

      BoxLayout:
         orientation:'vertical'
         size_hint_x: 0.35
         canvas.before:
            Color:
               rgba: app.color_secondary
            Rectangle:
               pos:self.pos
               size: self.size

         AnchorLayout:
            size_hint_y:None
            anchor_y:"top"
#            padding: dp(24)
            anchor_x:"left"
            Text:

               text: "POS SYSTEM"
               font_size: app.fonts.size.h2
               valign: "top"
               halign: "center"
               color: app.color_primary_text

            Image:
               source:"assets/imgs/logo-light.png"
         BoxLayout:
            orientation:'vertical'

            MDToolbar:
               title:"Bottom Navbar"

            MDBottomNavigation:

               MDBottomNavigationItem:
                  name:"screen 1"
                  text: "Dashboard"

#            Button:
#               text: "Dashboard"
#            Button:
#               text: "Tables"
#            Button:
#               text:"Staff"




      BoxLayout:
         orientation:'horizontal'


