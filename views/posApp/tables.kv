
<TableUILight@BoxLayout>
    padding:'30dp'
    spacing:'20dp'
    orientation:'vertical'
    canvas.before:
        Color:
            rgb: rgba(144, 245, 208, 0.2)
        RoundedRectangle:
            pos:self.pos
            size:self.size

<TableUIDark@BoxLayout>
    padding:'30dp'
    spacing:'20dp'
    orientation:'vertical'
    canvas.before:
        Color:
            rgb: rgba(128, 172, 237, 0.1)
        RoundedRectangle:
            pos:self.pos
            size:self.size

<ButtonTableLight@MDRectangleFlatIconButton>

    icon: "table-chair"
    text_color: "black"
    line_color:rgba(78, 47, 227, 0.2)
    theme_icon_color: "Custom"
    icon_color: "orange"
    pos_hint: {"center_x": 0.5, "center_y": 0.5}

<ButtonTableDark@MDRectangleFlatIconButton>
    icon: "table-chair"
    text_color: "black"
    line_color:rgba(29, 14, 102,0.2)
    theme_icon_color: "Custom"
    icon_color: "orange"
    pos_hint: {"center_x": 0.5, "center_y": 0.5}


<Tables>:

    TableUIDark:
        ButtonTableDark:
            text:"TABLE1"

    TableUILight:
