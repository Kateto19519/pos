#: import FadeTransition  kivy.uix.screenmanager.FadeTransition
#: import menu views.menu.menu
#: import homeadmin views.home_admin.home_admin
<MainWindow>:
    orientation: 'vertical'
    canvas.before:
        Color:
            rgba: app.color_primary_bg
        Rectangle:
            pos: self.pos
            size: self.size
    ScreenManager:
        id: scrn_mngr
        transition: FadeTransition()
        Screen:
            name: 'scrn_auth'
            Auth:
                id: auth
        Screen:
            name:'scrn_menu'
            Menu:
                id:menu
        Screen:
            name: 'scrn_home'
            Home:
                id: home
        Screen:
            name:'home_admin'
            HomeAdmin:
                id:home_admin






